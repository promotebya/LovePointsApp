{
  "expo": {
    "name": "LovePointsApp",
    "slug": "LovePointsApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.promotebya.lovepoints",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.promotebya.lovepoints"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "useModularHeaders": true
          }
        }
      ],
      "expo-splash-screen"
    ],
    "extra": {
      "eas": {
        "projectId": "d791c6d2-2f30-4b8e-b5b6-0f52eb421bf3"
      }
    }
  }
}
